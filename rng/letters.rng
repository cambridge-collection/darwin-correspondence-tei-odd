   <define name="tei_orgName">
      <element name="orgName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(organization name) contains an organizational name. [13.2.2. Organizational Names]</a:documentation>
         <ref name="tei_macro.phraseSeq"/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tei_math-orgName-orgName-link-check-constraint-rule-48">
            <rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:svg="http://www.w3.org/2000/svg"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  context="tei:orgName[ancestor::tei:correspAction][normalize-space(@key)]">
               <let name="document_uri"
                    value="string-join(tokenize(document-uri(/),'/')[position() != last()],'/')"/>
               <let name="namereg_full_path" value="concat($document_uri,'/',@key)"/>
               <assert test="doc-available($namereg_full_path)">ERROR: <value-of select="$namereg_full_path"/>
                  <name/> points to incorrect or missing namereg file.</assert>
               <assert test="matches(normalize-space(@key),'nameregs_\d+\.xml$')">ERROR: Invalid @key value of '<value-of select="@key"/>' for <name/>.</assert>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tei_math-orgName-orgName-needs-key-attr-constraint-rule-49">
            <rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:svg="http://www.w3.org/2000/svg"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  context="tei:orgName[ancestor::tei:correspAction][not(ancestor::tei:teiHeader//tei:change[@status='cancelled'])]">
               <assert test="normalize-space(@key)">element <name/> missing required attribute 'key'</assert>
            </rule>
         </pattern>
         <optional>
            <attribute name="key">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Points to the namereg authority file for the organisation - i.e. "../nameregs/nameregs_979.xml"</a:documentation>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="tei_persName">
      <element name="persName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(personal name) contains a proper noun or proper-noun phrase referring to a person, possibly including one or more of the person's forenames, surnames, honorifics, added names, etc. [13.2.1. Personal Names]</a:documentation>
         <ref name="tei_macro.phraseSeq"/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tei_math-persName-persName-link-check-constraint-rule-50">
            <rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:svg="http://www.w3.org/2000/svg"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  context="tei:persName[ancestor::tei:correspAction][normalize-space(@key)]">
               <let name="document_uri"
                    value="string-join(tokenize(document-uri(/),'/')[position() != last()],'/')"/>
               <let name="namereg_full_path" value="concat($document_uri,'/',@key)"/>
               <assert test="doc-available($namereg_full_path)">ERROR: <value-of select="$namereg_full_path"/>
                  <name/> points to incorrect or missing namereg file.</assert>
               <assert test="matches(normalize-space(@key),'nameregs_\d+\.xml$')">ERROR: Invalid @key value of '<value-of select="@key"/>' for <name/>.</assert>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tei_math-persName-persName-needs-key-attr-constraint-rule-51">
            <rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:svg="http://www.w3.org/2000/svg"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  context="tei:persName[ancestor::tei:correspAction][not(ancestor::tei:teiHeader//tei:change[@status='cancelled'])]">
               <assert test="normalize-space(@key)">element <name/> missing required attribute 'key'</assert>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tei_math-persName-persName-exclude-must-point-to-name-in-same-context-constraint-rule-52">
            <rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:svg="http://www.w3.org/2000/svg"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  context="tei:persName[@exclude]">
               <let name="xmlid" value="concat('#',@xml:id)"/>
               <let name="exclude_vals" value="tokenize(normalize-space(@exclude),' ')"/>
               <let name="sibling_name_elem_xmlids"
                    value="parent::*/tei:*[self::tei:persName|self::tei:orgName|self::tei:name]/concat('#',@xml:id)"/>
               <report test="count($exclude_vals[not(. = $sibling_name_elem_xmlids)]) != 0">ERROR: <name/>@exclude (<value-of select="$exclude_vals[not(. = $sibling_name_elem_xmlids)]"/>) does not point to a name in the same correspAction.</report>
               <report test="exists($sibling_name_elem_xmlids[not(.= $xmlid)][not(. = $exclude_vals)])">ERROR: <name/>'s @exclude does not point to (<value-of select="$sibling_name_elem_xmlids[not(.= $xmlid)][not(. = $exclude_vals)]"/>).</report>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tei_math-persName-persName-exclude-points-to-self-constraint-rule-53">
            <rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:svg="http://www.w3.org/2000/svg"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  context="tei:persName[@exclude]">
               <report test="normalize-space(@exclude) != '' and tokenize(@exclude,'\s+')=concat('#',@xml:id)">ERROR: <name/>@exclude value (<value-of select="concat('#',@xml:id)"/>) points to same element as itself</report>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tei_math-persName-persName-exclude-not-xmlid-constraint-rule-54">
            <rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:svg="http://www.w3.org/2000/svg"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  context="tei:persName[@exclude]">
               <report test="normalize-space(@exclude) != '' and not(@xml:id[normalize-space(.)])">ERROR: <name/> needs @xml:id</report>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tei_math-persName-persName-xmlid-not-exclude-constraint-rule-55">
            <rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:svg="http://www.w3.org/2000/svg"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  context="tei:persName[@xml:id]">
               <report test="normalize-space(@xml:id) != '' and not(@exclude[normalize-space(.)])">ERROR: <name/> only needs @xml:id if it's an @exclude option.</report>
            </rule>
         </pattern>
         <optional>
            <attribute name="xml:id">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(identifier) provides a unique identifier for the name. Should only be used when it is one of a mutually exclusive set of names.</a:documentation>
               <data type="ID"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="exclude">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">points to elements that are in exclusive alternation with the current element.</a:documentation>
               <list>
                  <oneOrMore>
                     <data type="anyURI">
                        <param name="pattern">\S+</param>
                     </data>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <optional>
            <attribute name="cert">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(certainty) signifies the degree of certainty associated with the intervention or interpretation.</a:documentation>
               <choice>
                  <data type="double"/>
                  <choice>
                     <value>high</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                     <value>medium</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                     <value>low</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                     <value>unknown</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  </choice>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="resp">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(responsible party) indicates the agency responsible for the intervention or interpretation, for example an editor or transcriber.</a:documentation>
               <list>
                  <oneOrMore>
                     <data type="anyURI">
                        <param name="pattern">\S+</param>
                     </data>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <optional>
            <attribute name="key">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Points to the namereg authority file for the individual - i.e. "../nameregs/nameregs_1.xml"</a:documentation>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
