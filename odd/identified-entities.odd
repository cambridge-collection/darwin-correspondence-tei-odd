            <elementSpec ident="quote" module="core" mode="change" xml:base="elements-lett/quote.xml" xml:lang="">
               <!--<gloss versionDate="2014-06-17" xml:lang="en"></gloss>-->
               <!--<desc versionDate="2014-06-17" xml:lang="en"></desc>-->
