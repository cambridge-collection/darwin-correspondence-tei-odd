                     <publicationStmt>
                        <publisher>CUP</publisher>
                        <pubPlace>Cambridge</pubPlace>
                        <date when="2015">2015</date>
                     </publicationStmt>
                  </egXML>
               </exemplum>
               <remarks versionDate="2015-08-16" xml:lang="en">
                  <p>This element is generated automatically before transcription begins.</p>
