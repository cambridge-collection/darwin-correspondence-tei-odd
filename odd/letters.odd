                           <assert test="matches(normalize-space(@key),'nameregs_\d+\.xml$')">ERROR: Invalid @key value of '<value-of select="@key"/>' for <name/>.</assert>
                        </rule>
                     </constraint>
                  </constraintSpec>
                  <constraintSpec ident="persName-needs-key-attr" scheme="schematron">
                     <constraint>
                        <rule xmlns="http://purl.oclc.org/dsdl/schematron" context="tei:persName[ancestor::tei:correspAction][not(ancestor::tei:teiHeader//tei:change[@status='cancelled'])]">
                           <assert test="normalize-space(@key)">element <name/> missing required attribute 'key'</assert>
                        </rule>
                     </constraint>
                  </constraintSpec>
                  <constraintSpec ident="persName-exclude-must-point-to-name-in-same-context" scheme="schematron">
                     <constraint>
                        <rule xmlns="http://purl.oclc.org/dsdl/schematron" context="tei:persName[@exclude]">
                           <let xmlns="http://purl.oclc.org/dsdl/schematron" name="xmlid" value="concat('#',@xml:id)"/>
                           <let xmlns="http://purl.oclc.org/dsdl/schematron" name="exclude_vals" value="tokenize(normalize-space(@exclude),' ')"/>
                           <let xmlns="http://purl.oclc.org/dsdl/schematron" name="sibling_name_elem_xmlids" value="parent::*/tei:*[self::tei:persName|self::tei:orgName|self::tei:name]/concat('#',@xml:id)"/>
                           <report xmlns="http://purl.oclc.org/dsdl/schematron" test="count($exclude_vals[not(. = $sibling_name_elem_xmlids)]) != 0">ERROR: <name/>@exclude (<value-of select="$exclude_vals[not(. = $sibling_name_elem_xmlids)]"/>) does not point to a name in the same correspAction.</report>
                           <report xmlns="http://purl.oclc.org/dsdl/schematron" test="exists($sibling_name_elem_xmlids[not(.= $xmlid)][not(. = $exclude_vals)])">ERROR: <name/>'s @exclude does not point to (<value-of select="$sibling_name_elem_xmlids[not(.= $xmlid)][not(. = $exclude_vals)]"/>).</report>
                        </rule>
                     </constraint>
                  </constraintSpec>
                  <constraintSpec ident="persName-exclude-points-to-self" scheme="schematron">
                     <constraint>
                        <rule xmlns="http://purl.oclc.org/dsdl/schematron" context="tei:persName[@exclude]">
                           <report xmlns="http://purl.oclc.org/dsdl/schematron" test="normalize-space(@exclude) != '' and tokenize(@exclude,'\s+')=concat('#',@xml:id)">ERROR: <name/>@exclude value (<value-of select="concat('#',@xml:id)"/>) points to same element as itself</report>
                        </rule>
                     </constraint>
                  </constraintSpec>
                  <constraintSpec ident="persName-exclude-not-xmlid" scheme="schematron">
