                  <constraintSpec ident="name-in-change-xmlid" scheme="schematron">
                     <constraint>
                        <rule xmlns="http://purl.oclc.org/dsdl/schematron" context="tei:name[ancestor::tei:change]">
                           <report test="not(@xml:id) and not(@sameAs)">ERROR: <name/> needs @xml:id or @sameAs</report>
                        </rule>
