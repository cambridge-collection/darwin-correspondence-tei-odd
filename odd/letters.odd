               <elementSpec ident="note" module="core" mode="change">
                  <gloss versionDate="2014-06-09" xml:lang="en">note</gloss>
                  <desc versionDate="2014-06-09" xml:lang="en">records either an editorial note in the header, descrption, source of text, footnotes, CD Notes or Annotations.</desc>
                  <classes mode="change">
                     <memberOf key="att.placement" mode="delete"/>
                     <memberOf key="att.written" mode="delete"/>
                  </classes>
                  <constraintSpec ident="no-type-in-text" scheme="schematron">
                     <constraint>
                        <rule xmlns="http://purl.oclc.org/dsdl/schematron" context="tei:note[ancestor::tei:text]">
                           <report test="@type">ERROR: <name/>@type should not be specified on Footnotes, CD Notes or Annotations.</report>
                        </rule>
                     </constraint>
                  </constraintSpec>
                  <constraintSpec ident="invalid-note-type" scheme="schematron">
                     <constraint>
                        <rule xmlns="http://purl.oclc.org/dsdl/schematron" context="tei:note[@type]">
                           <report test="not(@type=('physdesc','description', 'postmark', 'endorsement','datestamp'))">ERROR: <name/>@type must equal 'physdesc', 'description', 'postmark', 'endorsement' or 'datestamp'</report>
                        </rule>
                     </constraint>
                  </constraintSpec>
                  <constraintSpec ident="footnote-not-target-transcription" scheme="schematron">
                     <constraint>
                        <rule xmlns="http://purl.oclc.org/dsdl/schematron" context="tei:note[@xml:id][parent::tei:div[@type='footnotes']]">
                           <report test="not(concat('#',@xml:id) = ancestor::tei:TEI/(tei:teiHeader|tei:text/tei:body/tei:div[@type=('letter','translation')])//tei:ref/@target)">ERROR: <name/> '<value-of select="@xml:id"/>' is not the target of a ref/@target in the letter transcription or header</report>
                        </rule>
                     </constraint>
                  </constraintSpec>
                  <constraintSpec ident="check-type-physdesc" scheme="schematron">
                     <constraint>
                        <rule xmlns="http://purl.oclc.org/dsdl/schematron" context="tei:note[@type]">
                           <report test="@type and not(ancestor::tei:teiHeader/tei:fileDesc/tei:notesStmt)">ERROR: <name/>@type='<value-of select="@type"/>' must be placed within notesStmt.</report>
                        </rule>
                     </constraint>
                  </constraintSpec>
                  <attList>
                     <attDef ident="n" usage="opt" mode="change"/>
                     <attDef ident="subtype" mode="change">
                        <desc xml:lang="en" versionDate="2016-06-30">describes the subtype of endorsement.</desc>
                     </attDef>
                     <attDef ident="target" mode="change">
                        <desc xml:lang="en" versionDate="2023-06-12">should not be used [legacy code].</desc>
                     </attDef>
                     <attDef ident="targetEnd" mode="change">
                        <desc xml:lang="en" versionDate="2023-06-12">should not be used [legacy code].</desc>
                     </attDef>
                     <attDef ident="type" usage="opt" mode="change">
                        <desc xml:lang="en" versionDate="2016-06-30">describes the type of notes (used only on metadata within teiHeader).</desc>
                        <valList type="semi" mode="replace">
                           <valItem ident="datestamp"/>
                           <valItem ident="description"/>
                           <valItem ident="endorsement"/>
                           <valItem ident="physdesc"/>
                           <valItem ident="postmark"/>
                        </valList>
                     </attDef>
                     <attDef ident="xml:id" mode="change">
                        <desc xml:lang="en" versionDate="2023-06-12">contains a unique identifier for the note. Only necessary on footnotes.</desc>
                     </attDef>
                     <attDef ident="ana" mode="delete"/>
                     <attDef ident="anchored" mode="delete"/>
                     <attDef ident="cert" mode="delete"/>
                     <attDef ident="change" mode="delete"/>
                     <attDef ident="copyOf" mode="delete"/>
                     <attDef ident="corresp" mode="delete"/>
                     <attDef ident="evaluate" mode="delete"/>
                     <attDef ident="exclude" mode="delete"/>
                     <attDef ident="facs" mode="delete"/>
                     <attDef ident="next" mode="delete"/>
                     <attDef ident="prev" mode="delete"/>
                     <attDef ident="rend" mode="delete"/>
                     <attDef ident="rendition" mode="delete"/>
                     <attDef ident="resp" mode="delete"/>
                     <attDef ident="sameAs" mode="delete"/>
                     <attDef ident="select" mode="delete"/>
                     <attDef ident="source" mode="delete"/>
                     <attDef ident="style" mode="delete"/>
                     <attDef ident="synch" mode="delete"/>
                     <attDef ident="targetLang" mode="delete"/>
                     <attDef ident="xml:base" mode="delete"/>
                     <attDef ident="xml:lang" mode="delete"/>
                     <attDef ident="xml:space" mode="delete"/>
                  </attList>
                  <remarks><p><gi>note</gi> fulfills many purposes in the DarML. In the header, it can take the attribute <att>physdesc</att> or <att>description</att> that contains a description of the original source of the text (in most cases from the Calendar) or an abbreviated description that would apprear in the print volume, respectively. <att>postmark</att> contains the text of any postamrks and <att>encorsement</att> occurs where the recipient of the letter endorsed it with a date of receipt. These all occur in the header grouped in <gi>notesStmt</gi>.</p>
                     <p>Footnotes are also marked with <gi>note</gi>, and wrapped in <tag>div type="footnotes"</tag>. Each footnote <gi>note</gi> should have an <att>xml:id</att> that connects to a <att>ref</att> somewhere in the text.</p>
                     <p>Finally, notes are used to indicate CD notes and annotations, which are laid out in a way designed to represent CD annotations and alterations to the text in the way they would have apeared in the print volumes.</p></remarks>
                  <remarks xml:lang="en" versionDate="2023-06-12">
                     <p><att>target</att> and <att>targetEnd</att> are legacy encodings that appear in some of our earliest files. They are used to record the start and end of the text in the transcription/translation to which the annotation applies. The accuracy of anchoring string that starts the annotation note (<hi rend="italic">i.e.</hi> everything before the ‘<val>]</val>’ is uncertain. Ideally, an editor would check and update these values so that they conform to the standards of the rest of the files.</p>
                  </remarks>
               </elementSpec>
               
               <elementSpec ident="p" module="core" mode="change">
                  <desc xml:lang="en" versionDate="2014-05-26">marks paragraphs in prose. No attributes are normally needed but <att>rend</att> (rendition) is available and it is used to indicate (in the case of textual transcriptions) how the item was rendered in the original text. Its values are "<val>right</val>" (right-aligned), "<val>left</val>" (left-aligned), "<val>centre</val>" (centred), or "<val>blockquote</val>". If <att>rend</att> is omitted, the paragraph will be output according to the destination format's (ie. print or web) default paragraph styles.</desc>
                  <classes mode="change">
                     <memberOf key="att.declaring" mode="delete"/>
                     <memberOf key="att.fragmentable" mode="delete"/>
                     <memberOf key="att.written" mode="delete"/>
                  </classes>
                  <attList>
                     <attDef ident="rend" mode="change">
                        <valList type="closed" mode="replace">
                           <valItem ident="right"/>
                           <valItem ident="left"/>
                           <valItem ident="centre"/>
                           <valItem ident="blockquote"/>
                        </valList>
                     </attDef>
                     <attDef ident="ana" mode="delete"/>
                     <attDef ident="cert" mode="delete"/>
                     <attDef ident="change" mode="delete"/>
                     <attDef ident="copyOf" mode="delete"/>
                     <attDef ident="corresp" mode="delete"/>
                     <attDef ident="exclude" mode="delete"/>
                     <attDef ident="facs" mode="delete"/>
                     <attDef ident="n" mode="delete"/>
                     <attDef ident="next" mode="delete"/>
                     <attDef ident="prev" mode="delete"/>
                     <attDef ident="rendition" mode="delete"/>
                     <attDef ident="resp" mode="delete"/>
                     <attDef ident="sameAs" mode="delete"/>
                     <attDef ident="select" mode="delete"/>
                     <attDef ident="source" mode="delete"/>
                     <attDef ident="style" mode="delete"/>
                     <attDef ident="synch" mode="delete"/>
                     <attDef ident="xml:base" mode="delete"/>
                     <attDef ident="xml:id" mode="delete"/>
                     <attDef ident="xml:lang" mode="delete"/>
                     <attDef ident="xml:space" mode="delete"/>
                  </attList>
                  <exemplum xml:lang="en" versionDate="2014-06-10">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <p>P.S.&#160;Upon the whole the Cirripedia show more affinity, <hi rend="underline">I think</hi>, to the Upper Chalk, than to any other stage— <hi rend="underline">pray</hi> tell me whether Upper Chalk fossils are generally more or less abundant than those of the Lower Chalk &amp;amp; Chalk Marl</p>
                     </egXML>
                  </exemplum>
                  <exemplum xml:lang="en">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <p rend="centre">From the Insects to their friend, Charles Darwin.</p>
                     </egXML>
                  </exemplum>
                  <remarks versionDate="2014-05-21" xml:lang="en">
                     <p><att>rend</att> is primarily to be used to record how the text was rendered in the original document. While it <hi rend="bold">may</hi> be used to force certain renderings on the outputted versions that were not present in the original, editors should contact Michael Hawkins should they feel the desire to do so.</p>
                     <p><att>rend</att> has not been given the value "<val>justified</val>" (where both the left and right hand sides of the texts are flush with the margin) since value was not used within DarXML.</p>
                  </remarks>
               </elementSpec>
               
               <elementSpec ident="publisher" module="core" mode="change">
                  <desc versionDate="2015-08-16" xml:lang="en">gives the name of the organisation responsible for the volume of The Correspondence of Charles Darwin containing this letter. This is invariably ‘Cambridge University Press’.</desc>
                  <classes mode="change">
                     <memberOf key="att.canonical" mode="delete"/>
